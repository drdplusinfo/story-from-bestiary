{
  "name": "drdplus/rules-skeleton",
  "license": "MIT",
  "type": "composer-plugin",
  "description": "Skeleton for DrD+ rules as HTML",
  "keywords": [
    "drd plus",
    "rules",
    "helper",
    "skeleton"
  ],
  "autoload": {
    "psr-4": {
      "DrdPlus\\": "DrdPlus/"
    }
  },
  "require": {
    "php": ">=7.1",
    "ext-mbstring": "*",
    "phpgt/dom": "~1.0",
    "piwik/device-detector": "^3.9",
    "tracy/tracy": "^2.4",
    "phpmailer/phpmailer": "^6.0",
    "composer-plugin-api": "^1.0",
    "drdplus/frontend-skeleton": "~1.0"
  },
  "require-dev": {
    "composer/composer": "^1.7",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~7.0",
    "roave/security-advisories": "dev-master",
    "drdplus/rules-skeleton-web": "dev-master"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "class": "DrdPlus\\RulesSkeleton\\SkeletonInjectorComposerPlugin"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
